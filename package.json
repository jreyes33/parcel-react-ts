{
  "name": "parcel-react-ts",
  "version": "1.0.0",
  "main": "index.tsx",
  "private": true,
  "scripts": {
    "check": "npm run format:check && npm run tsc && npm run build",
    "build": "parcel build --public-url . index.html",
    "format": "prettier --write '**/*.ts?(x)'",
    "format:check": "prettier --check '**/*.ts?(x)'",
    "start": "parcel serve index.html",
    "tsc": "tsc --noEmit"
  },
  "license": "CC0-1.0",
  "devDependencies": {
    "@types/node": "^12.12.30",
    "@types/parcel-env": "0.0.0",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5",
    "sass": "^1.26.10",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "formik": "^2.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "prettier": {
    "semi": false
  }
}
